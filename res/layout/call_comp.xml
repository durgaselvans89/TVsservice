<?xml version="1.0" encoding="utf-8"?>
<ScrollView
		xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
	 		android:scrollbars="none">
	<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:paddingBottom="10dip"
			android:background="@drawable/bg">
			<ImageView
				android:id="@+id/imgViewHeader_05"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/call_complete_header"/>	
		 <LinearLayout
		 	android:id="@+id/linearSERHeader_05"
		 	android:layout_below="@id/imgViewHeader_05"
			android:orientation="vertical"
		 	android:layout_width="fill_parent"
			android:layout_height="wrap_content" 
			android:layout_marginTop="10dip">
			
		     <TextView android:id="@+id/txtSERNUMBER_05" android:layout_width="fill_parent" android:layout_height="wrap_content"
			    android:layout_centerHorizontal="true"
			    android:textSize="22sp"
				android:textStyle="bold"
				android:typeface="sans"
				android:textColor="#FFFFFF"
				android:gravity="center"/>
			
	   		<TextView 
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"  
				android:layout_marginTop="10dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
				android:text="Engineer Name"/>
			<TextView 
				android:layout_width="fill_parent" 
				android:layout_height="40sp"
				android:layout_marginTop="5dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:padding="10dip"
				android:textSize="14sp"
				android:singleLine="true"
				android:background="@drawable/textbox"
				android:id="@+id/edtxtEngineerName_05" 
				android:text="Name"
				android:textColor="#000000"/>
			<TextView 
				android:layout_width="fill_parent" 
				android:layout_marginTop="5dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
				android:layout_height="wrap_content" 
				android:text="Technician ID"></TextView>
			<TextView 
				android:layout_width="fill_parent" 
				android:id="@+id/edtxtTechID_05" 
				android:layout_marginTop="5dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:text="Tech ID"
				android:textColor="#000000"
				android:padding="10dip"
				android:background="@drawable/textbox"
				android:textSize="14sp"
				android:singleLine="true"
				android:layout_height="40sp"/>
			
			<TextView 
				android:layout_width="fill_parent" 
				android:layout_marginTop="10dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
				android:layout_height="wrap_content" 
				android:text="Result"/>		
			<Spinner 
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content" 
				android:prompt="@string/result"			
				android:entries="@array/Result"
				android:paddingLeft="15dip"
				android:layout_marginTop="5dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:id="@+id/spinResult_05"/>
			
			<TextView 
				android:layout_width="fill_parent" 
				android:layout_marginTop="5dip"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
				android:layout_height="wrap_content" 
				android:text="Comp.DT/Time"></TextView>
				<RelativeLayout android:layout_marginTop="5dip"
				    android:layout_width="wrap_content" 
				    android:layout_height="wrap_content">
					<TextView
				 			android:id="@+id/txtDate_05"
				 			android:layout_width="wrap_content"
				 			android:layout_height="wrap_content"
				 			android:layout_marginLeft="10dip"
				 			android:textColor="#000000"
				 			android:textSize="14sp"
				 			android:gravity="center_vertical"
				 			android:hint="Select date"
				 			android:padding="10dip"
				 			android:background="@drawable/date_box"/>
				 		<TextView
				 		    android:layout_toRightOf="@+id/txtDate_05"
				 			android:id="@+id/txtTime_05"
				 			android:layout_width="wrap_content"
				 			android:layout_height="wrap_content"
				 			android:layout_marginLeft="20dip"
				 			android:textColor="#000000"
				 			android:gravity="center_vertical"
				 			android:hint="Select Time"
				 			android:padding="10dip"
				 			android:textSize="14sp"
				 			android:background="@drawable/time_box"/>
			</RelativeLayout>
		
		<Button 
			android:layout_width="fill_parent" 
			android:id="@+id/btnPartsTransaction_05" 
			android:layout_marginTop="15dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:layout_height="wrap_content"
			android:gravity="left|center_vertical" 
			android:textSize="14sp"
			android:background="@drawable/spinner_right"
			android:paddingLeft="10dip"
			android:text="Parts Transaction"/>
		<LinearLayout android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginTop="10dip"
		 	android:orientation="horizontal">
		<LinearLayout android:id="@+id/cold_Layout" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:orientation="vertical">
			<TextView 
		 			android:layout_width="wrap_content"
		 			android:layout_height="wrap_content"
		 			android:textColor="#ffffff"
		 			android:layout_gravity="center_horizontal"
		 			android:textSize="14sp"
		 			android:text="Cold Boot Done"/>
		 			        <RadioGroup android:orientation="horizontal"
					        android:layout_width="wrap_content"
					        android:layout_gravity="center_horizontal"
					        android:layout_height="wrap_content" >
					        <RadioButton 
					            android:id="@+id/radiocooldDone"
					            android:layout_width="wrap_content"
					            android:layout_height="wrap_content"
					            android:checked="false" 
					            android:text="Yes"/>
					 
					        <RadioButton android:layout_marginLeft="20dip"
					            android:id="@+id/radiocooldNo"
					            android:layout_width="wrap_content"
					            android:layout_height="wrap_content"
					            android:checked="false" 
					            android:text="No"/>
 
    						</RadioGroup>
		 			    </LinearLayout>
		 			    <LinearLayout android:layout_width="wrap_content" android:id="@+id/Poh_Layout"
			android:layout_height="wrap_content" android:orientation="vertical">
		 		<TextView 
		 			android:layout_width="wrap_content"
		 			android:layout_height="wrap_content"
		 			android:textColor="#ffffff"
		 			android:textSize="14sp"
		 			android:layout_gravity="center_horizontal"
		 			android:text="POH"/>	
		 			        <RadioGroup android:layout_gravity="center_horizontal"
					        android:layout_width="wrap_content"
					        android:layout_height="wrap_content" android:orientation="horizontal">
					 
					        <RadioButton 
					            android:id="@+id/radioPOHDone"
					            android:layout_width="wrap_content"
					            android:layout_height="wrap_content"
					            android:checked="false" 
					            android:text="Yes"/>
					 
					        <RadioButton android:layout_marginLeft="20dip"
					            android:id="@+id/radioPOHNo"
					            android:layout_width="wrap_content"
					            android:layout_height="wrap_content"
					            android:checked="false" 
					            android:text="No"/>
 
    						</RadioGroup>
		 			    </LinearLayout>
		</LinearLayout>
		<TextView
 			android:layout_width="fill_parent"
 			android:layout_height="wrap_content"
 			android:layout_marginTop="10dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
 			android:textColor="#ffffff"
 			android:textSize="14sp"
 			android:text="DTS Name"/>
		<EditText 
			android:layout_width="fill_parent" 
			android:id="@+id/edtxtDtsName_05" 
			android:imeOptions="actionNext"
			android:hint="DTS Name" 
			android:textSize="14sp"
			android:singleLine="true"
			android:layout_marginTop="5dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:layout_height="wrap_content"/>
		    
		<TextView 
			android:layout_width="fill_parent" 
			android:layout_marginTop="10dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:textColor="#ffffff"
		 	android:textSize="14sp"
			android:layout_height="wrap_content" 
			android:text="Case ID" />
		<EditText 
			android:layout_width="fill_parent" 
			android:id="@+id/edTxtCaseId_05" 
			android:imeOptions="actionNext"
			android:layout_marginTop="5dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:hint="Case ID" 
			android:singleLine="true"
			android:textSize="14sp"
			android:layout_height="wrap_content"/>
	
		<TextView 
				android:layout_width="wrap_content" 
				android:text="Call Wrap-up / System Check" 
				android:textColor="#ffffff"
			 	android:textSize="14sp"
				android:singleLine="true"
				android:layout_marginTop="10dip"
				android:layout_marginLeft="10dip"
				android:layout_height="wrap_content"/>
		<RadioGroup android:layout_marginLeft="30dip"
		android:layout_marginTop="5dip"
        android:id="@+id/radioEsurvey_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:orientation="horizontal">
 
	        <RadioButton android:layout_marginLeft="10dip"
	            android:id="@+id/radioEsurvey_05Done"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:checked="false" 
	            android:text="Yes"/>
	 
	        <RadioButton android:layout_marginLeft="30dip"
	            android:id="@+id/radiocEsurvey_05No"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:checked="false" 
	            android:text="No"/>

		</RadioGroup>
		<TextView 
				android:layout_width="wrap_content" 
				android:layout_marginTop="10dip"
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="Customer Satisfied"/>
		<RadioGroup android:layout_marginLeft="30dip"
			android:layout_marginTop="5dip"
	        android:id="@+id/radiocustomer_satisfied_05"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" android:orientation="horizontal">
	 
		        <RadioButton android:layout_marginLeft="10dip"
		            android:id="@+id/radiocustomer_satisfied_05Done"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:checked="false" 
		            android:text="Yes"/>
		 
		        <RadioButton android:layout_marginLeft="30dip"
		            android:id="@+id/radioccustomer_satisfied_05No"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:checked="false" 
		            android:text="No"/>

		</RadioGroup>
		<Button
			android:id="@+id/btnActDone_05" 
			android:background="@drawable/bsc_spinner_gender"
			android:layout_centerHorizontal="true"
			android:imeOptions="actionNext"
			android:text="Activity Done"
			android:textSize="16sp"
	        android:typeface="sans" 
	        android:textColor="#000000" 
	        android:ellipsize="marquee"	
			android:gravity="left|center_vertical"
			android:layout_marginTop="5dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:paddingRight="40dip"
			android:layout_width="fill_parent"
			android:layout_height="50dip"/>

		<Button
			android:id="@+id/btnAdditionalActtivityDone_05" 
			android:background="@drawable/bsc_spinner_gender"
			android:layout_centerHorizontal="true"
			android:imeOptions="actionNext"
			android:text="Additional Activity"
			android:textSize="16sp"
	        android:typeface="sans" 
	        android:textColor="#000000" 
	        android:ellipsize="marquee"	
			android:gravity="left|center_vertical"
			android:layout_marginTop="5dip"
			android:paddingLeft="10dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:paddingRight="40dip"
			android:layout_width="fill_parent"
			android:layout_height="50dip"/>
		
		<EditText 
			android:layout_width="fill_parent" 
			android:id="@+id/edTxtActivityDone_05" 
			android:imeOptions="actionDone"
			android:layout_marginTop="5dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:hint="Provide additional inputs if any" 
			android:gravity="left|top"
			android:textSize="14sp"
			android:layout_height="120dip"/>
		<RelativeLayout android:layout_width="wrap_content" android:layout_marginTop="10dip"
		    android:layout_height="wrap_content">
		    <TextView android:layout_centerVertical="true"
		        android:id="@+id/txtAcceptance_05"
				android:layout_width="wrap_content" 
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="E-ROF Acceptance: "/>
		    <CheckBox android:layout_toRightOf="@id/txtAcceptance_05"
	        android:id="@+id/chkAcceptance_05"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="Accept" />
		</RelativeLayout>
		
		<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent"
		    android:id="@+id/linearLayout_Acceptance_05" android:orientation="vertical" android:visibility="gone">
		    <TextView 
				android:layout_width="wrap_content" 
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
				android:layout_height="wrap_content"
				android:text="DISCLAIMER: By Selecting the &quot;Accept&quot; option, you are signing this document electronically. You agree your electronic signature is legally equivalent of your manual signature in  this agreement. By selecting the accept option you consent to be legally bound by this agreement's terms and conditions."
				/>
		    <LinearLayout android:layout_marginTop="20dip" android:layout_width="fill_parent" 
		        android:layout_height="wrap_content"
		        android:orientation="horizontal">
		    	<TextView 
				android:layout_width="120dip" 
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="Customer Name"/>
		        <EditText android:layout_width="fill_parent"
		            android:singleLine="true"
		            android:id="@+id/edtCustomer_name_05"
		            android:imeOptions="actionDone"
		            android:textSize="14sp" android:layout_marginRight="10dip"
					android:layout_height="wrap_content"/>
		    </LinearLayout>
		    
	        <LinearLayout android:layout_marginTop="10dip" android:layout_width="fill_parent" 
		        android:layout_height="wrap_content"
		        android:orientation="horizontal">
		    	<TextView 
				android:layout_width="120dip" 
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="Customer Mail-Id"/>
		        <EditText android:layout_width="fill_parent"
		            android:singleLine="true"
		            android:id="@+id/edtCustomer_mailId_05"
		            android:imeOptions="actionDone"
		            android:inputType="textEmailAddress"
		            android:textSize="14sp" android:layout_marginRight="10dip"
					android:layout_height="wrap_content"/>
		    </LinearLayout>
		    
            <LinearLayout android:layout_marginTop="10dip" android:layout_width="fill_parent" 
		        android:layout_height="wrap_content"
		        android:orientation="horizontal">
		    	<TextView 
				android:layout_width="120dip" 
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="Customer job-Title"/>
		        <EditText android:layout_marginRight="10dip" android:layout_width="fill_parent"
		            android:id="@+id/edtCustomer_jobTitle_05"
		            android:imeOptions="actionDone"
		            android:singleLine="true"
		            android:textSize="14sp"
					android:layout_height="wrap_content"/>
		    </LinearLayout>
		    
		</LinearLayout>	
		
	    <TextView  android:layout_marginTop="10dip"
				android:layout_width="wrap_content" 
				android:layout_marginLeft="10dip"
				android:textColor="#ffffff"
			 	android:textSize="14sp"
			 	android:singleLine="true"
				android:layout_height="wrap_content" 
				android:text="Customer Comments"/>
		        <EditText android:layout_width="fill_parent"
		            android:id="@+id/edtCustomer_commants_05"
		            android:imeOptions="actionDone"
		            android:maxLength="350"
		            android:gravity="left|top"
		            android:layout_marginLeft="10dip"
					android:layout_marginRight="10dip"
		            android:textSize="14sp"
					android:layout_height="120dip"
					android:hint="Text Input"/>
		        
		<Button
			android:id="@+id/buttonSign_05" 
			android:layout_centerHorizontal="true"
			android:textSize="15sp"
	        android:typeface="sans" 
	        android:textColor="#000000" 
	        android:imeOptions="actionDone"
	        android:layout_marginTop="5dip"
			android:layout_marginLeft="10dip"
			android:layout_marginRight="10dip"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:layout_width="fill_parent" 
			android:text="Customer Signature"/>		
	</LinearLayout>
	<Button
			android:layout_below="@+id/linearSERHeader_05"  
			android:id="@+id/buttonSubmit_05" 
			android:layout_centerHorizontal="true"
			android:textSize="16sp"
 			android:textStyle="bold"
	        android:typeface="sans" 
	        android:textColor="#000000" 
			android:layout_height="wrap_content"
			android:background="@drawable/button"
			android:gravity="center"
			android:layout_marginTop="10dip" 
			android:layout_width="wrap_content" 
			android:text="Submit"/>	
</RelativeLayout>
</ScrollView>			