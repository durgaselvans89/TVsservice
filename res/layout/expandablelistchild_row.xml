<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/item_bg">
   <!-- <TextView android:id="@+id/grp_child"
         android:paddingLeft="50px"
         android:textSize="14px"
         android:text="child"
         android:textColor="#333333"
         android:gravity="center_vertical"
         android:textStyle="normal"
         android:layout_width="150px"
         android:layout_height="35dip"/>-->
     <RelativeLayout
     			android:id="@+id/exListChild_02"
		    	android:layout_width="fill_parent"
		    	android:layout_height="wrap_content"> 	
		    	
		    <ImageView
				android:id="@+id/imgViewCall_02"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/view"
				android:layout_alignParentRight="true"
				android:layout_marginTop="5dip"
				android:layout_toRightOf="@+id/txtSerialNo_02"
				android:layout_marginRight="10dip"/>	
			<TextView 
				android:layout_marginTop="5dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 
				android:layout_marginLeft="10dip" 
				android:id="@+id/txtSER_Header_02"
				android:text="SER: "
				android:textSize="14sp"
				android:textStyle="bold"
				android:typeface="sans"
				android:textColor="#333333"/>
					   	  
			<TextView 
				android:layout_marginTop="5dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 
				android:paddingRight="10dip"
				android:layout_marginRight="20dip"
				android:layout_toRightOf="@+id/txtSER_Header_02"
				android:id="@+id/txtSER_02"
				android:text="Number"
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>
				
			 <TextView 
				android:layout_marginTop="5dip"
				android:layout_marginBottom="5dip"
				android:layout_width="wrap_content"
				android:layout_below="@+id/txtSER_02"
				android:layout_height="wrap_content" 
				android:layout_marginLeft="10dip" 
				android:id="@+id/txtOrg_Header_02"
				android:textStyle="bold"
				android:text="Org Name: "
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>
				
			<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:paddingRight="10dip"
				android:layout_marginTop="5dip"
				android:layout_toRightOf="@+id/txtOrg_Header_02"
				android:layout_below="@+id/txtSER_02"
				android:id="@+id/txtOrgName_02"
				android:text="Park Limited"
				android:layout_marginBottom="5dip"
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>
			
			<TextView 
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/txtOrgName_02"
				android:layout_marginLeft="10dip" 
				android:id="@+id/txtContactHeader_02"
				android:text="Contact Name: "
				android:textStyle="bold"
				android:layout_marginBottom="5dip"
				android:visibility="visible"
				android:textSize="14sp"
				android:typeface="sans"
			android:textColor="#333333"/>

			<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:paddingRight="10dip"
				android:layout_below="@+id/txtOrgName_02"
				android:layout_toRightOf="@+id/txtContactHeader_02"
				android:visibility="visible"
				android:id="@+id/txtContactName_02"
				android:text="Dilip"
				android:layout_marginBottom="5dip"
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>
				
			<TextView 
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dip"
				android:layout_below="@+id/txtContactName_02"
				android:layout_marginLeft="10dip" 
				android:id="@+id/txtApptDateTime_02"
				android:text="Customer ETA DT: "
				android:textStyle="bold"
				android:layout_marginBottom="5dip"
				android:visibility="visible"
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>
		
		<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:paddingRight="10dip"
				android:layout_marginTop="5dip"
				android:layout_below="@+id/txtContactName_02"
				android:layout_toRightOf="@+id/txtApptDateTime_02"
				android:visibility="visible"
				android:id="@+id/txtAppDateTime_02"
				android:text=""
				android:textSize="14sp"
				android:typeface="sans"
				android:textColor="#333333"/>		
			
		 <ImageView
				android:id="@+id/imgViewCall_02"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="15dip"
				android:src="@drawable/call_icon"
				android:layout_marginLeft="10dip" 
				android:layout_below="@id/txtAppDateTime_02"/>
					
			<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:paddingRight="20dip"
				android:paddingLeft="5dip"
				android:paddingTop="10dip"
				android:paddingBottom="10dip"
				android:layout_below="@+id/txtAppDateTime_02"
				android:layout_toRightOf="@+id/imgViewCall_02"
				android:visibility="visible"
				android:id="@+id/txtPhoneNo_02"
				android:text="9800416286"
				android:textSize="14sp"
				android:textStyle="bold"
				android:typeface="sans"
				android:textColor="#6AB01C"/>
		</RelativeLayout>		      
</LinearLayout>